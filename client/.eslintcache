S:\n        case LOGIN_SUCCESS:\n            localStorage.setItem('token', payload.token);\n            return {...state, ...payload, isAuthenticated: true, loading: false};\n        \n        case REGISTER_FAIL:\n        case AUTH_ERROR:\n        case LOGIN_FAIL:\n        case LOGOUT:\n        case ACCOUNT_DELETED:\n            localStorage.removeItem('token');\n            return {...state, token: null, isAuthenticated: false, loading: false};\n\n        default:\n            return state;\n    }\n}","/home/sonia/Documents/Sonia/Intern-Sonia/kalaseii/client/src/reducers/profile.js",["157"],"import { GET_PROFILE, PROFILE_ERROR, CLEAR_PROFILE, UPDATE_PROFILE, GET_PROFILES } from \"../actions/types\";\n\nconst initialState ={\n    profile: null,\n    profiles: [],\n    loading: true,\n    error: {}\n}\n\nexport default function(state = initialState, action){\n    const { type, payload } = action;\n\n    switch(type){\n        case GET_PROFILE:\n        case UPDATE_PROFILE:\n            return {\n                ...state,\n                profile: payload,\n                loading: false\n            }\n        case GET_PROFILES:\n            return{\n                ...state,\n                profiles: payload,\n                loading: false\n            }\n        case PROFILE_ERROR:\n            return {\n                ...state,\n                error: payload,\n                loading: false\n            }\n        case CLEAR_PROFILE:\n            return {\n                ...state,\n                profile: null,\n                loading: false\n            }\n\n        default:\n            return state;\n    }\n}",{"ruleId":"158","replacedBy":"159"},{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","severity":1,"message":"163","line":45,"column":7,"nodeType":"164","endLine":45,"endColumn":35,"suggestions":"165"},{"ruleId":"166","severity":1,"message":"167","line":4,"column":1,"nodeType":"168","endLine":8,"endColumn":2},{"ruleId":"166","severity":1,"message